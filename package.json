{
  "name": "@modelcontextprotocol/conformance",
  "version": "0.1.7",
  "type": "module",
  "license": "MIT",
  "author": "Anthropic, PBC (https://anthropic.com)",
  "homepage": "https://modelcontextprotocol.io",
  "bugs": "https://github.com/modelcontextprotocol/conformance/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modelcontextprotocol/conformance.git"
  },
  "scripts": {
    "start": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsdown src/index.ts --minify --clean --target node20",
    "lint": "eslint src/ examples/ && prettier --check .",
    "lint:fix": "eslint src/ examples/ --fix && prettier --write .",
    "lint:fix_check": "npm run lint:fix && git diff --exit-code --quiet",
    "check": "npm run typecheck && npm run lint",
    "typecheck": "tsgo --noEmit",
    "prepack": "npm run build"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "conformance": "dist/index.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/express": "^5.0.3",
    "@types/node": "^22.10.2",
    "@typescript/native-preview": "^7.0.0-dev.20251030.1",
    "cors": "^2.8.5",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.1.8",
    "lefthook": "^2.0.2",
    "prettier": "3.6.2",
    "tsdown": "^0.15.12",
    "tsx": "^4.7.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "8.46.2",
    "vitest": "^4.0.5"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0-beta.0",
    "commander": "^14.0.2",
    "express": "^5.1.0",
    "zod": "^3.25.76"
  }
}
